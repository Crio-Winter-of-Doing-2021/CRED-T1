    <script src="..\..\assets\libs\@fancyapps\fancybox\dist\jquery.fancybox.min.js"></script>
    <script src="..\..\assets\js\quick-website.js"></script>
    <script src="assets/libs/sweetalert2/dist/sweetalert2.min.js"></script>

